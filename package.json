{
  "name": "@acrois/circular-slider",
  "version": "0.0.4",
  "author": {
    "name": "Aaron Croissette",
    "email": "hello@acrois.dev",
    "url": "https://acrois.dev/"
  },
  "description": "A customizable circular slider web component designed for accessibility.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/circular-slider/circular-slider.esm.js",
  "exports": {
    ".": {
      "import": "./dist/circular-slider/circular-slider.esm.js",
      "require": "./dist/circular-slider/circular-slider.cjs.js"
    },
    "./circular-slider": {
      "import": "./dist/components/circular-slider.js",
      "types": "./dist/components/circular-slider.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "bugs": {
    "url": "https://github.com/acrois/circular-slider/issues"
  },
  "homepage": "https://github.com/acrois/circular-slider#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrois/circular-slider.git"
  },
  "files": [
    "dist/",
    "loader/",
    "readme.md"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^22.12.1"
  },
  "keywords": [
    "stencil",
    "web-components",
    "circular-slider"
  ],
  "license": "MIT"
}
